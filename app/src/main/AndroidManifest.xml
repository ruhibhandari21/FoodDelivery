<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cloverinfosoft.fooddelivery">
<!--alias:FoodDelivery pass:keystore-->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".initials.LoginActivity"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:screenOrientation="portrait"
            android:name=".initials.ForgotPasswordActivity" />
        <activity
            android:screenOrientation="portrait"
            android:name=".dashboard.DashboardActivity" />
        <activity
            android:screenOrientation="portrait"
            android:name=".dashboard.AddToCartActivity" />
        <activity
            android:screenOrientation="portrait"
            android:name=".dashboard.MyOrderActiviy"></activity>
    </application>

</manifest>